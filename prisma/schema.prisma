generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Enquiry {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  name      String
  email     String
  subject   String
  message   String
  isSolved  Boolean  @default(false)
  remarks   String?
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
